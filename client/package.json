    "eslint-webpack-plugin": "^3",
    "favicons-webpack-plugin": "^5",
    "fork-ts-checker-webpack-plugin": "^7",
    "html-loader": "^3",
    "html-webpack-plugin": "^5",
    "jest": "^28",
    "mini-css-extract-plugin": "^2",
    "postcss": "^8",
    "postcss-loader": "^7",
    "prettier": "^2",
    "replace-in-files-cli": "^2",
    "resolve-url-loader": "^5",
    "sass": "^1",
