{
  "name": "powerlevel-client",
  "version": "0.1.0",
  "description": "PowerLevel web client",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack --watch --config webpack.config.js",
    "start-pwa": "cross-env NODE_ENV=development PWA_DEV=true webpack --watch --config webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "test": "cross-env NODE_ENV=development jest --passWithNoTests",
    "test-watch": "cross-env NODE_ENV=development jest --passWithNoTests --watchAll",
    "stylelint": "stylelint \"**/*.(css|scss)\"",
    "lint": "eslint ./",
    "profile": "cross-env NODE_ENV=production webpack --profile --config webpack.config.js --json=./dist/profile.json && webpack-bundle-analyzer ./dist/profile.json",
    "postinstall": "replace-in-files --string='sourceMappingURL' --replacement='_' ./node_modules/browser-sync-client/dist/index.js"
  },
  "dependencies": {
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fontsource/roboto": "^4",
    "@linaria/core": "3.0.0-beta.15",
    "@mui/icons-material": "^5",
    "@mui/material": "^5",
    "@reduxjs/toolkit": "^1",
    "react": "^18",
    "react-dom": "^18",
    "react-redux": "^8",
    "react-router-dom": "^6"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/plugin-transform-runtime": "^7",
    "@babel/preset-env": "^7",
    "@babel/preset-react": "^7",
    "@babel/preset-typescript": "^7",
    "@linaria/babel-preset": "3.0.0-beta.15",
    "@linaria/shaker": "3.0.0-beta.15",
    "@linaria/stylelint": "^3.0.0-beta.15",
    "@linaria/webpack-loader": "^3.0.0-beta.16",
    "@testing-library/dom": "^8",
    "@testing-library/jest-dom": "^5",
    "@testing-library/react": "^13",
    "@testing-library/user-event": "^14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "babel-loader": "^8",
    "browser-sync-webpack-plugin": "^2",
    "clean-webpack-plugin": "^4",
    "connect-history-api-fallback": "^1",
    "core-js": "^3",
    "cross-env": "^7",
    "css-loader": "^6",
    "cssnano": "^5",
    "eslint": "^8",
    "eslint-config-airbnb": "^19",
    "eslint-config-airbnb-base": "^15",
    "eslint-config-airbnb-typescript": "^17",
    "eslint-config-prettier": "^8",
    "eslint-import-resolver-alias": "^1",
    "eslint-plugin-eslint-comments": "^3",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jest": "^26",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-prettier": "^4",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^4",
